library(readxl)
library(ggplot2)
library(dplyr)
library(ggstatsplot)

################################### Warm-up period ###########################

ggplot(data = WIP, aes(x = Hora, y = Pacientes, color =Replica)) + 
  geom_line() + geom_vline(xintercept = 168, col="red", size=1) +
  theme_bw()+labs(x = "Hora de la simulaci?n",y = "Pacientes en el servicio")+
  ggtitle("Gr?fico para determinar el Warm-up period") +
  facet_wrap(vars(Covid),scales = "fixed")

################################## Cycle Time ############################################

## An?lisis de Triage ##

ggplot(data = CycleTime, aes(x = Paciente, y = Horas, color = Paciente)) + 
  geom_boxplot() +
  theme_bw()+labs(x = "Tipo de Paciente",y = "Tiempo en sistema (horas)")+
  ggtitle("Tiempo en sistema por tipo de paciente en todos los escenarios")

ggplot(data = CycleTime, aes(x = Horas, color = Paciente)) +
  geom_density()+
  geom_histogram(fill="white",aes(y=..density..), position="identity", alpha=0.2)+
  ggtitle("Tiempo en sistema por tipo de paciente en todos los escenarios")

ggplot(data = CycleTime, aes(x = Horas, color = Paciente)) + 
  geom_density()+
  labs(x = "Tipo de Paciente",y = "Tiempo (horas)")+
  ggtitle("Tiempo en sistema por tipo de paciente en todos los escenarios")

##Interacci?n Tipo de Paciente y Covid###
ggplot(data = CycleTime, aes(x = Paciente, y = Horas, color = Covid)) + 
  geom_boxplot() +
  theme_bw()+labs(x = "Tipo de Paciente",y = "Tiempo (horas)")+
  ggtitle("Tiempo en sistema por tipo de paciente por escenario")

ggplot(data = CycleTime, aes(x = Covid, y = Horas, colour = Paciente,
                             group = Paciente)) +
  stat_summary(fun = mean, geom = "point") +
  stat_summary(fun = mean, geom = "line") +
  labs(y  =  "Tiempo en sistema (horas)", x="Proporci?n pacientes Covid") +
  theme_bw()+
  ggtitle("Tiempo en sistema por tipo de paciente por escenario")

##Interacci?n Hora y D?a##
ggplot(data = CycleTime, aes(x = Dia, y = Horas, color = Turno)) + 
  geom_boxplot() +
  theme_bw()+labs(x = "D?a",y = "Tiempo (horas)")+
  ggtitle("Tiempo en sistema por d?a y turno")

ggplot(data = CycleTime, aes(x = Turno, y = Horas, colour = Dia,
                             group = Dia)) +
  stat_summary(fun = mean, geom = "point") +
  stat_summary(fun = mean, geom = "line") +
  labs(y  =  "Tiempo en sistema (horas)", x="Turno") +
  theme_bw()+
  ggtitle("Tiempo en sistema por d?a y turno")

ggplot(data = CycleTime, aes(x = Turno, y = Horas, colour = Dia,
                             group = Dia)) +
  stat_summary(fun = mean, geom = "point") +
  stat_summary(fun = mean, geom = "line") +
  labs(y  =  "Tiempo en sistema (horas)", x="Turno") +
  theme_bw()+
  ggtitle("Tiempo en sistema por d?a, turno, tipo de paciente y escenario") +
  facet_wrap(vars(Paciente,Covid), scales = "fixed")+ 
  theme(axis.text.x = element_text(angle=30, vjust = 0.7))


###An?lisis de Pacientes Respiratorio###

ggplot(data = CycleTimeR, aes(x = Covid, y = Horas, color = Covid)) + 
  geom_boxplot() +
  theme_bw()+labs(x = "Porcentaje de Covid",y = "Tiempo (horas)")+
  ggtitle("Tiempo en sistema paciente Respiratorio por escenario")

ggplot(data = CycleTimeR, aes(x = Horas, color = Covid)) +
  geom_density()+
  geom_histogram(fill="white",aes(y=..density..), position="identity", alpha=0.2)+
  labs(x = "Tiempo (horas)")+
  ggtitle("Tiempo en sistema paciente Respiratorio por escenario")

ggplot(data = CycleTimeR, aes(x = Horas, color = Covid)) + 
  geom_density()+
  labs(x = "Tiempo (horas)")+
  ggtitle("Tiempo en sistema paciente Respiratorio por escenario")

##An?lisis de turno Respiratorio##

ggplot(data = CycleTimeR, aes(x = Turno, y = Horas, color = Turno)) + 
  geom_boxplot() +
  theme_gray()+labs(x = "Turno",y = "Tiempo (horas)")+
  ggtitle("Tiempo en sistema paciente Respiratorio por Turno")


ggplot(data = CycleTimeR, aes(x = Horas, color = Turno)) + 
  geom_histogram(fill="white",aes(y=..density..), position="identity", alpha=0.2)+
  labs(x = "Tiempo (horas)")+
  ggtitle("Tiempo en sistema paciente Respiratorio por Turno")

ggplot(data = CycleTimeR, aes(x = Horas, color = Turno)) + 
  geom_density()+ labs(x = "Tiempo (horas)")+
  ggtitle("Tiempo en sistema paciente Respiratorio por Turno")


##An?lisis de d?a Respiratorio##
ggplot(data = CycleTimeR, aes(x = Dia, y = Horas, color = Dia)) + 
  geom_boxplot() +
  theme_bw()+labs(x = "D?a",y = "Tiempo (horas)")+
  ggtitle("Tiempo en sistema paciente Respiratorio por D?a")


ggplot(data = CycleTimeR, aes(x = Horas, color = Dia)) + 
  geom_density()+ theme_bw()+labs(x = "D?a",y = "Tiempo (horas)")+
  ggtitle("Tiempo en sistema paciente Respiratorio por D?a")


